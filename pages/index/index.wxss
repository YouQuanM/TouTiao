page{  
  display: flex;  
  flex-direction: column;  
  height: 100%;  
  background: #fff;
}  
.pre_load{
  position:absolute;
  top:0;
  left:0;
  z-index:10;
  width:100%;
  height:100%;
}

.pre_load image{
  height: 100%;
}

.navbar{  
  flex: none;  
  display: flex;  
  background: #fff;  
  position: relative;
  top: 0rpx;
  font-size:30rpx;
  color: #666;
  height:170rpx;
  padding-top:20rpx;
  border-bottom:20rpx solid #F7F8F9;
}  
.hezu-part,.zhengzu-part,.yezhu-part{  
  position: relative;  
  flex: 1;  
  text-align: center;
}  
/* .navbar .item.active{
  color: #3dbcc5;
} */

.down_city{
  width: 16rpx;
  height: 11rpx;
}

.scroll-part{
  position:absolute;
  top: 0rpx;
  bottom: 0rpx;
  width: 100%;
}

.hz-img{
  width: 100%;
  height: 120rpx;
  position:relative;
  top:0rpx; 
}

.index-img{
  border-radius: 8rpx; 
  width:690rpx;
  height:390rpx;
}

.hezu{
   background: #fff;
}

.index-model{
  /* border-bottom:1px solid #e5e5e5; */
  padding-bottom:80rpx;
  margin: 30rpx;
  
}

.hr{
  width: 100%;
  height: 2rpx;
  background: #EBEBEB;
  margin-top:-30rpx;

}

.index-address{
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  display: flex;
  flex-direction: row;
}

.local-img{
  width:22rpx;
  height:24rpx;
  position:relative;
  top:4rpx;
}

.index-subway{
  float: left;
  position: relative;
  top: 15rpx;
  font-size: 24rpx;
  color: #999;
}

.index-price{
  float:right;
  position:relative;
  left:-8rpx;
  color:#FF7966;
  font-size:32rpx;
  top:7rpx;
  font-weight:bold;
}

.index-month{
  color: #666;
  font-size: 24rpx;
}

 .tag-part{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
} 

.tag-text{
  font-size:30rpx;
  font-weight:bold;
  margin-bottom:30rpx;
  display:block;
}

.btn-select{
  border:1rpx solid #3dbcc5;
  width:128rpx;
  font-size:24rpx;
  color: #3dbcc5;
  border-radius:50rpx;
  padding:10rpx 10rpx;
  margin-right:20rpx;
  margin-bottom:30rpx;
  text-align:center;
} 

.btn-select:active{
  color: #fff;
  background: #3dbcc5;
}



.actTag{
  border:1rpx solid #3dbcc5;
  background: #3dbcc5;
  width:129rpx;
  font-size:24rpx;
  color: #fff;
  border-radius:50rpx;
  padding:10rpx 10rpx;
  margin-right:20rpx;
  margin-bottom:30rpx;
  text-align:center;
} 

.tryTag{
  margin-bottom:35rpx;
