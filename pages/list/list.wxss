page{
  width:100%;
	height: 100%;
	color: #333;
  display: flex;
  flex-direction: column;
  overflow: hidden
}

.blue{
  color:#3dbcc5;
}

.container-body{
  position:absolute;
  top:201rpx;
  bottom:0rpx;
  width:100%;
  background: #fff;
  padding:24rpx 24rpx 0 24rpx;
  box-sizing: border-box;
}

.list-Nav{
  display:flex;
  flex-direction:row;
  position:fixed;
  width: 100%;
  top:113rpx;
  border-bottom:1px solid #f2f4f6;
  font-size:30rpx;
  color:#666;
  height:100rpx;
  background:#fff;
  z-index:2;

}

.Nav-tag{
  position:relative;
  width: 25%;
  text-align: center;
  line-height: 100rpx;

}
.Nav-tag text{
  display: block;
  width:75%;
  overflow: hidden; 
  white-space: nowrap; 
  text-overflow: ellipsis;
}

.nav-icon{
  position:absolute;
  right:30rpx;
  top:50%;
  width: 0;
  height: 0;
  margin-top:-4rpx;
  border: 8rpx solid transparent;
  border-top: 8rpx solid #999;
}

.icon-hzz{
  margin-left:150rpx;
}

.room-detail{
  padding:30rpx 0rpx;
  border-bottom:1px solid #f7f8f9;
  display: flex;
  position:relative;
}
.room-detail view{
   box-sizing: border-box;
}

.room-img{
  position: relative;
}

.room-img image{
  width: 260rpx;
  height: 180rpx;
}

.room-img view{
  position:absolute;
  width:38rpx;
  background:#3dbcc5;
  font-size:24rpx;
  color:#fff;
  top:0rpx;
  border-radius:0rpx 0rpx 20rpx 0;
  padding-left:4rpx;
  height:37rpx;
  opacity:0.8;
}

.roomOther-info{
  position:relative;
  padding-right:20rpx;
  margin-left:30rpx;
  font-size: 26rpx;
}

.other-title{
  width:420rpx;
  margin-right:30rpx;
  font-size:30rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow:hidden;
  color:#000;
}

.detail-info{
  margin-top: 15rpx;
  padding-right: 24rpx; 
}

.detail-info image{
  width:25rpx;
  height:27rpx;
}

.detail-info text{
  margin-left:15rpx;
}

.distence-info{
  display:block;
  margin-top:5rpx;
 width:420rpx;
 overflow: hidden;
 text-overflow: ellipsis;
 white-space: nowrap;
}

.distence-info image{
  width:24rpx;
  height:25rpx;
}

.distence-info text{
  margin-left:15rpx;
}

.otherroom-tag{
  display:flex;
  font-size:22rpx;
  margin-top:10rpx;

}

.otherroom-tag view{
  border:1px solid #bfc4c5;
  color:#a9afb1;
  padding:5rpx 10rpx;
  margin-right:20rpx;
}

.room-price{
  position:absolute;
  top:80rpx;
  right:0;
  font-size:26rpx;
  color:#333;
}

.room-price text{
  font-size: 28rpx;
  color: #FF7966;
}
.layer-box,.layer-box view,.layer-box text{
  box-sizing: border-box;
}
.layer-box{
  position:absolute;
  top: 216rpx;
  right:0;
  left:0;
  bottom:0;
  z-index: 2;
  background:rgba(0,0,0,.5);
  overflow: auto;
}

.layer-box>view{
   position:relative;
   font-size:28rpx;
   line-height:80rpx;
   text-align:center;
   background:#fff;
   color:#666;
}
.layer-box>view::after{
  clear: both;
}

/*筛选-租住类型  */
.layer-rent{
  padding:30rpx 0;
  line-height:80rpx;
}

/* 筛选-位置  */
.layer-pos-box{
  display: flex;
  width:100%;
  max-height:80%;
}
.layer-pos-box>view,.layer-pos-box>scroll-view{
  flex:1;
  line-height:80rpx;
}
.layer-pos-box .first{
  padding:30rpx 0;
}
.layer-pos-box .second{
  padding:30rpx 0;
   background:#f2f2f2;
}


/* 筛选-更多  */
.layer-box .layer-more{
  text-align:left;
  height:70%;
  max-height:70%;
}
.layer-more .type-title{
  padding:0 30rpx;
  font-size:32rpx;
  
  font-weight: bold;
}
.layer-more scroll-view{
   height:100%;
}
.layer-more .type-list{
  padding:0 12rpx;
  border-bottom:1px solid #f2f2f2;
}
.layer-more .type-list text{
  display:inline-block;
   width:160rpx;
   height:55rpx; 
   line-height:53rpx;
   margin-left:18rpx;
   border:1rpx solid #eee;
   border-radius:100px;
   text-align: center;
   overflow: hidden;
}
.layer-more .type-list.col-3 text{
   width:220rpx;
}
.layer-more .type-list text.on{
   background:#dff9f9;
   color:#33a7b4;
   border-color:#dff9f9;
}
/* //slider */
.custom {
  margin:30rpx 30rpx 0;
  padding-top: 15rpx;
  border-top: 1rpx solid #eee;
}
.custom::after{
   overflow: hidden
}
.slider-box{
  position:relative;
  width:280px;
  height:120rpx;
  margin:0 auto;
  padding:30rpx 0;
}
.slider-box .slider{
  position:absolute;
  width:20px;
  height:20px;
  top:50%;
  margin-top: -10px;
  border-radius:50%;
  background:#3dbcc6;
  z-index: 4
}
.slider-box .slider.s1{
  left:0;
  margin-left:-10px;
}
.slider-box .slider.s2{
  right:0;
  margin-right:-10px;
}
.slider-box .blueline{
   position:absolute;
   top: 50%;
   height:4rpx;
   width:100%;
   margin-top: -2rpx;
   background:#3DBCC6;
   z-index: 2
}
.slider-box .grayline{
   position:absolute;
   top: 50%;
   height:4rpx;
   margin-top: -2rpx;
   background:#ccc;
   z-index: 3;
}
.slider-box .grayline.l2{
  right:0;
}
.slider-box .grayline.l1{
  left:0;
}
.both-price {
  color:#3dbcc6;
}

/* 筛选按钮  */
.submit-btn{
  position: absolute;
  left:0;
  top:100%;
  width:100%;
   display: flex;
   line-height:80rpx;
   background:#fff;
   box-shadow: 0px -3rpx 8rpx rgba(0,0,0,.1);
   text-align:center;
}
.submit-btn .reset{
   flex:1;
}
.submit-btn .sure{
   flex:2;
   background:#3dbcc6;
   color:#fff;
}

/*no room  */
.no_room{
   margin: 50rpx 0 0 0;
}
.no_room image{
  width:350rpx;
  height:482rpx;
  display: block;
  margin:0 auto;
}

.recommend{
  background:#F3F4F6;
  text-align:center;
  height:65rpx;
  line-height:60rpx;
  color:#666;
  margin:20rpx 0; 
}

.recommend image{
  width:24rpx;
  height:24rpx;
  position:relative;
  left:-20rpx;
  top:4rpx;
}

.recommend view{
  display:inline-block;
  font-size:28rpx;
}

.top-site{
  background:#F8F8F8;
  box-shadow: 0 0 0 0;
  padding: 15rpx 30rpx;
}

.search{
  top:-15rpx;
}

.sch-ipt{
  background: #EAEAEA;
  padding-left:65rpx;
}

.search image{
  left: -5rpx;
}

.listInt{
  color: #999;
}
